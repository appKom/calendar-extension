{
    "name": "Online i Google Calendar",
    "description": "Utvidelse for Ã¥ automatisk legge til Online-arrangementer i Google Calendar",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },

    "background": {
        "service_worker": "background.js",
        "permission": [
            "tabs",
            "activeTab"
